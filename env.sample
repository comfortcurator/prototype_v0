# Copy this file to `.env.local` (development) and configure the values before running `pnpm dev`.

# Core database & auth
DATABASE_URL="postgresql://user:password@host:5432/project_v0"
DIRECT_URL="postgresql://user:password@host:5432/project_v0"
NEXTAUTH_SECRET="replace-with-secure-random-string"
NEXTAUTH_URL="http://localhost:3000"

# Supabase
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"

# Providers
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your-google-maps-key"
RAZORPAY_KEY_ID="rzp_test_key"
RAZORPAY_KEY_SECRET="rzp_test_secret"
RAZORPAY_WEBHOOK_SECRET="rzp_test_webhook"
NEXT_PUBLIC_RAZORPAY_KEY_ID="rzp_test_key"

# Stripe (optional future integration)
STRIPE_SECRET_KEY="sk_test_..."
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# Logging / analytics
LOG_LEVEL="info"
SENTRY_DSN=""
VERCEL_ANALYTICS_ID=""

